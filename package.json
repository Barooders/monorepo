{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "Barooders monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "format": "prettier \"packages/shared-types/**/*.ts\" \"packages/backend/**/*.ts\" \"packages/frontend/**/*.ts\" \"packages/store/**/*.ts\" --check",
    "format:fix": "prettier --write \"packages/shared-types/**/*.ts\" \"packages/backend/**/*.ts\"  \"packages/frontend/**/*.ts\" \"packages/store/**/*.ts\"",
    "install-for-render": "./install-for-render.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/lodash": "4.14.198",
    "@types/node": "18.14.0",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "newrelic": "11.14.0",
    "react": "18.2.0",
    "react-use": "17.4.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "glob": "10.3.10",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "lint-staged": "13.3.0"
  }
}
