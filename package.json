{
    "name": "monorepo",
    "version": "1.0.0",
    "description": "Barooders monorepo",
    "private": true,
    "workspaces": [
      "packages/*"
    ],
    "scripts": {
      "prebuild": "yarn workspace backend prebuild",
      "build": "yarn workspace backend build && yarn workspace backend prisma:migrate",
      "console": "yarn workspace backend console"
    },
    "cacheDirectories": [
      "node_modules",
      "packages/backend/node_modules",
      "packages/cypress/node_modules",
      "packages/frontend/node_modules"
    ],
    "dependencies": {
      "typescript": "5.3.3",
      "newrelic": "11.5.0",
      "react": "18.2.0",
      "react-use": "17.4.0",
      "@types/lodash": "4.14.198",
      "@types/node": "18.14.0",
      "eslint": "8.56.0",
      "dotenv": "16.3.1"
    },
    "devDependencies": {
      "@typescript-eslint/eslint-plugin": "7.0.2",
      "@typescript-eslint/parser": "7.0.2",
      "eslint-config-prettier": "8.6.0",
      "eslint-plugin-prettier": "4.2.1",
      "husky": "8.0.3",
      "lint-staged": "13.3.0",
      "prettier": "2.8.8"
    }
  }