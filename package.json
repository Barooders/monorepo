{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "Barooders monorepo",
  "private": true,
  "workspaces": [
    "packages/app-scripts",
    "packages/backend",
    "packages/cypress",
    "packages/frontend",
    "packages/hasura",
    "packages/scripts",
    "packages/shared-types"
  ],
  "scripts": {
    "format": "prettier \"{,**/}*.{json,ts,css,scss,yml,tsx}\" --check",
    "format:fix": "prettier --write \"{,**/}*.{json,ts,css,scss,yml,tsx}\"",
    "install-for-render": "./install-for-render.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/lodash": "4.14.198",
    "@types/node": "18.14.0",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "newrelic": "11.14.0",
    "react": "18.2.0",
    "react-use": "17.4.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "glob": "10.3.10",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "ts-node": "10.9.1",
    "ts-loader": "9.4.4",
    "tsconfig-paths": "4.1.0",
    "lint-staged": "13.3.0"
  }
}
