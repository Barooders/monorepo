meta {
  name: Get Homepage config
  type: graphql
  seq: 12
}

post {
  url: {{strapiBaseUrl}}/graphql
  body: graphql
  auth: none
}

body:graphql {
  query {
    webHome {
      data {
        attributes {
          Header {
          	... on ComponentMediaImageSection {
              link
              image {
                data {
                  attributes {
                    url
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
