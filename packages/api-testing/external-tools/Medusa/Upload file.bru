meta {
  name: Upload file
  type: http
  seq: 1
}

post {
  url: {{medusaUrl}}/admin/uploads
  body: multipartForm
  auth: none
}

headers {
  x-medusa-access-token: {{medusaAdminToken}}
}

body:multipart-form {
  files: @file(/Users/clementdessoude/Downloads/active_products_limit_photos.csv)
}

script:post-response {
  bru.setEnvVar("medusaFileKey",res?.body?.uploads?.[0]?.key)
}
