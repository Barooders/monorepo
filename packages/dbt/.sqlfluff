[sqlfluff]
dialect = postgres
templater = dbt
exclude_rules = layout.long_lines

[sqlfluff:indentation]
tab_space_size = 2

[sqlfluff:rules:capitalisation.keywords]
capitalisation_policy = upper

[sqlfluff:templater:dbt]
project_dir = .
profiles_dir = config/local
profile = barooders_dbt_postgres
target = staging
