{
  "name": "frontend",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "link-env": "ln -fs ../.env web/.env && ln -fs ../.env native/.env && ln -fs ../.env pwa/.env",
    "lint": "next lint native && next lint web && next lint pwa",
    "prettier:fix": "prettier --write \"**/*.*\"",
    "generate": "yarn generate:graphql && yarn generate:rest",
    "generate:rest": "openapi-typescript http://localhost:3000/api-yaml -o ./shared/__generated/rest-schema.d.ts",
    "generate:graphql": "graphql-codegen --config codegen.ts",
    "generate:styles": "tailwindcss -c ./tailwind.config.js -o ./web/public/static-tailwind.css",
    "assets:generate": "npx @capacitor/assets generate --iosProject native/ios/App --androidProject native/android --assetPath native/assets --iconBackgroundColor '#e51736' --splashBackgroundColor '#e51736' --ios --android",
    "dev:web": "yarn link-env && next dev web -p 3001",
    "build:web": "yarn generate:styles && next build web",
    "build:with-sourcemaps:web": "yarn build:web && sentry-cli sourcemaps inject web/.next && sentry-cli sourcemaps upload web/.next --release=$(git rev-parse HEAD) && find ./web/.next -name \"*.map\" -delete",
    "start:web": "NODE_OPTIONS='-r @newrelic/next' next start web -p ${PORT:-3001}",
    "init:native": "bundle install",
    "dev:native": "yarn link-env && next dev native -p 3001",
    "sync:native": "NODE_ENV=production next build native && cap sync",
    "start:native:ios": "cap run ios --target 599855D3-E66C-4B04-A08C-4421B2FDCC44",
    "deploy:native:ios": "yarn sync:native && bundle exec fastlane ios beta",
    "start:native:android": "cap run android",
    "deploy:native:android": "yarn sync:native && bundle exec fastlane android beta",
    "deploy:native": "./increment-version.sh && yarn deploy:native:ios && yarn deploy:native:android",
    "dev:pwa": "yarn link-env && next dev pwa -p 3001",
    "build:pwa": "NODE_ENV=production next build pwa",
    "start:pwa": "next start pwa -p ${PORT:-3001}",
    "deploy:storybook": "chromatic --project-token=904867aae543",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "resolutions": {
    "@types/react": "18.2.45"
  },
