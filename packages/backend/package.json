  "scripts": {
    "decrypt-secrets": "../../decrypt-secrets.sh",
    "prebuild": "rimraf dist && yarn run decrypt-secrets && yarn prisma:generate",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "scripts": "ts-node -r tsconfig-paths/register src/scripts.ts",
    "cron:dev": "ts-node -r tsconfig-paths/register src/cron.ts",
    "run:prod": "BAROODERS_ENV=production ts-node -r tsconfig-paths/register scripts/render-job/index.ts",
    "run:staging": "BAROODERS_ENV=staging ts-node -r tsconfig-paths/register scripts/render-job/index.ts",
    "update:conversations": "ts-node -r tsconfig-paths/register scripts/update-talkjs-conversation/index.ts",
    "import:customers": "ts-node -r tsconfig-paths/register scripts/sync-customer/index.ts",
    "update:tags": "ts-node -r tsconfig-paths/register scripts/tagging/update-tags.ts",
    "update:transmission-tag": "ts-node -r tsconfig-paths/register scripts/tagging/update-transmission-tag.ts",
    "update:metafield": "ts-node -r tsconfig-paths/register scripts/tagging/update-metafield.ts",
    "update:price": "ts-node -r tsconfig-paths/register scripts/price/update-price.ts",
    "create:orders": "ts-node -r tsconfig-paths/register scripts/create-orders/index.ts",
    "update:variant-price": "ts-node -r tsconfig-paths/register scripts/price/update-variant-price.ts",
    "update:products": "ts-node -r tsconfig-paths/register scripts/update-products/update-products.ts",
    "update:orders": "ts-node -r tsconfig-paths/register scripts/update-orders/update-orders.ts",
    "update:vendor-variants": "ts-node -r tsconfig-paths/register scripts/update-products/update-all-variants-of-vendor.ts",
    "fix:variants-weight": "ts-node -r tsconfig-paths/register scripts/update-products/fix-variants-weight.ts",
    "update:vendor-products": "ts-node -r tsconfig-paths/register scripts/update-products/update-all-products-of-vendor.ts",
    "update:stocks": "ts-node -r tsconfig-paths/register scripts/update-products/update-all-variants-stocks.ts",
    "archive:products": "ts-node -r tsconfig-paths/register scripts/update-products/archive-list-of-products.ts",
    "create:reviews": "ts-node -r tsconfig-paths/register scripts/review/create-reviews.ts",
    "find-circular-deps": "madge --ts-config tsconfig.json --circular --extensions ts src/",
    "validate-folder-structure": "folderslint",
    "console": "RUN_MODE=console node -r newrelic dist/src/console.js",
    "cron": "node -r newrelic dist/src/cron.js",
    "index:products": "yarn console indexProduct all && yarn console indexProduct prune --apply",
    "index:collections": "yarn console indexCollection all && yarn console indexCollection prune --apply",
    "index:all": "yarn index:products && yarn index:collections",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r newrelic dist/src/main",
    "start:consumer:dev": "ts-node -r tsconfig-paths/register src/consumer.ts",
    "start:consumer:prod": "node -r newrelic dist/src/consumer",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.ts",
    "prisma": "./bin/prisma",
    "prisma:migrate:dev": "yarn prisma all migrate dev",
    "prisma:migrate": "yarn prisma all migrate deploy",
    "prisma:generate": "yarn prisma all generate",
    "prisma:direct": "prisma",
    "shopify:dev": "shopify app dev",
    "shopify:deploy": "shopify app deploy",
    "update:dev": "yarn install && yarn prisma all migrate dev",
    "isomake:shopify": "node -r tsconfig-paths/register -r ts-node/register isomaker",
    "isomake:db": "node -r tsconfig-paths/register -r ts-node/register isomaker/syncCollectionsToDB"
  },
